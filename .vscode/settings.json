{
  "editor.formatOnSave": true,
  "javascript.autoClosingTags": true,
  "javascript.format.enable": false,
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#ff7300",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#ff7300af",
    "titleBar.inactiveForeground": "#ffffff"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.gitignore": false,
    "**/.nowignore": true,
    "**/.prettierrc": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.lock": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.vscode": true,
    "**/.next": true,
    "**/.eslint*": true,
    "**/jsconfig": true,
    "**/package.json": true
  }
}
